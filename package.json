{
  "name": "massive-ecommerce-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pre": "cross-env-shell yenv print env.yaml $NODE_ENV dotenv -f -s > .env",
    "dev": "cross-env NODE_ENV=development npm run pre && next dev",
    "build:dev": "cross-env NODE_ENV=development npm run pre && next build",
    "build:prod": "cross-env NODE_ENV=production npm run pre && next build",
    "start:dev": "cross-env NODE_ENV=development npm run pre && next start",
    "start:prod": "cross-env NODE_ENV=production npm run pre && next start",
    "lint": "next lint --fix",
    "interface": "i18next-resources-for-ts interface -i ./public/locales/en -o ./src/utils/resources.util.d.ts",
    "toc": "i18next-resources-for-ts toc -i ./public/locales/en -o ./src/utils/resources.util.ts",
    "prepare": "husky install"
  },
